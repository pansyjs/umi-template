{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "lib": ["esnext", "dom"],
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["./src/*"],
      "@@/*": ["./src/.umi/*"]
    }
  },
  "exclude": [
    "node_modules",
    "dist",
    "src/.umi/*",
    "src/.umi-production/*"
  ]
}
